{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "firstName",
        "lastName"
    ],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1
        },
        "password": {
            "type": "string",
            "minLength": 5
        },
        "firstName": {
            "type": "string",
            "minLength": 1
        },
        "lastName": {
            "type": "string",
            "minLength": 1
        }
    }
}